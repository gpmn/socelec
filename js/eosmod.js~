var eos = null;

function initEos(eosAddr, activeKey){
    //var Eos=require("node_modules/eosjs/lib/eos.js")
    console.log("exports.initEos begin");
    var options = {
        httpEndpoint   : eosAddr,
        keyProvider    : activeKey,
        expireInSeconds: 60,
        broadcast      : true,
        debug          : false,
        sign           : true
    };
    console.log("exports.initEos before Eos(options)");
    eos = Eos(options);
}

function getGames(){
    eos.getTableRows({
        code:"soccer",
        scope:"matches",
        table:"match",
        json:true,
    }).then(result => {console.log(result)})
}
